{
  "name": "@strawbees/code-ui",
  "version": "1.3.14-prerelease.1",
  "scripts": {
    "dev": "node lib/scripts/dev.js",
    "build": "node lib/scripts/build.js",
    "build-serve": "node lib/scripts/buildServe.js",
    "build-targets": "node lib/scripts/buildTargets.js",
    "i18n:scratch-blocks:pull": "node lib/scripts/i18nPull.js",
    "i18n:zanata:pull": "zanata-cli pull --username $ZANATA_USER --key $ZANATA_KEY --create-skeletons",
    "i18n:zanata:push": "zanata-cli push --username $ZANATA_USER --key $ZANATA_KEY --push-type both",
    "i18n:pull": "npm run i18n:zanata:pull && npm run i18n:scratch-blocks:pull",
    "deploy:stage": "CONFIG=web_stage node lib/scripts/deploy.js",
    "deploy:production": "CONFIG=web_production node lib/scripts/deploy.js",
    "prepublishOnly": "npm run build-targets",
    "lint": "eslint --fix ./*.js src/**/*.js pages/**/*.js lib/**/*.js"
  },
  "bin": {
    "strawbees-code-ui-build": "./lib/scripts/build.js"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@svgr/webpack": "^4.1.0",
    "file-saver": "^2.0.0",
    "informed": "1.10.12",
    "lz-string": "^1.4.4",
    "next": "^8.0.1",
    "next-redux-wrapper": "^3.0.0-alpha.1",
    "node-fetch": "^2.3.0",
    "platform": "^1.3.5",
    "pofile": "^1.0.11",
    "prop-types": "^15.7.2",
    "quirkbot-chrome-app": "^0.3.2",
    "react": "^16.8.2",
    "react-ace": "^6.4.0",
    "react-dom": "^16.8.2",
    "react-draggable": "^3.1.1",
    "react-dropzone": "^8.1.0",
    "react-ga": "^2.5.7",
    "react-load-script": "^0.0.6",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.0",
    "react-remarkable": "^1.1.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "s3": "^4.4.0",
    "scratch-blocks": "^0.1.0-prerelease.1549990124",
    "serve-handler": "^5.0.8",
    "strawbees-app-packager": "^1.4.5",
    "tinycolor2": "^1.4.1",
    "workbox-build": "^3.6.3",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
