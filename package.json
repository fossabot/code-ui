{
  "name": "@strawbees/code-ui",
  "version": "1.10.3",
  "scripts": {
    "dev": "node lib/scripts/dev.js",
    "build": "node lib/scripts/build.js",
    "build-serve": "node lib/scripts/buildServe.js",
    "build-targets": "node lib/scripts/buildTargets.js",
    "i18n:scratch-blocks:pull": "node lib/scripts/i18nPull.js",
    "i18n:zanata:pull": "zanata-cli pull --username $ZANATA_USER --key $ZANATA_KEY --create-skeletons --errors",
    "i18n:zanata:push:templates": "zanata-cli push --username $ZANATA_USER --key $ZANATA_KEY --errors",
    "i18n:zanata:push:translations": "zanata-cli push --username $ZANATA_USER --key $ZANATA_KEY --push-type trans --errors",
    "i18n:pull": "npm run i18n:zanata:pull && npm run i18n:scratch-blocks:pull",
    "prepublishOnly": "npm run build-targets",
    "lint": "eslint --fix ./*.js src/**/*.js pages/**/*.js lib/**/*.js"
  },
  "bin": {
    "strawbees-code-ui-build": "./lib/scripts/build.js"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@strawbees/quirkbot-chrome-app": "^0.3.4",
    "@svgr/webpack": "^5.1.0",
    "file-saver": "^2.0.2",
    "informed": "1.10.12",
    "lz-string": "^1.4.4",
    "next": "^9.2.0",
    "next-redux-wrapper": "^4.0.1",
    "node-fetch": "^2.6.0",
    "platform": "^1.3.5",
    "pofile": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-dropzone": "^10.2.1",
    "react-ga": "^2.7.0",
    "react-load-script": "^0.0.6",
    "react-markdown": "^4.3.1",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "scratch-blocks": "0.1.0-prerelease.1578322100",
    "serve-handler": "^6.1.2",
    "tinycolor2": "^1.4.1",
    "workbox-build": "^4.3.1",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
